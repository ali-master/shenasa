{
	"printWidth": 100,
	"trailingComma": "all",
	"singleQuote": false,
	"semi": true,
	"tabWidth": 2,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"arrowParens": "always",
	"overrides": [
		{
			"files": [
				"*.json",
				"*.jsonc"
			],
			"options": {
				"parser": "json"
			}
		}
	]
}
